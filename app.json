{
  "expo": {
    "name": "PiggyWallet",
    "slug": "MeuApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": {
        "dark": "./assets/ios_icon.png",
        "light": "./assets/ios_icon.png",
        "tinted": "./assets/ios_icon.png"
      },
      "bundleIdentifier": "com.PiggyWallet",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0C4777"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "monochromeImage": "./assets/adaptive-icon.png"
      },
      "package": "com.PiggyWallet",
      "permissions": [
        "CAMERA",
        "FLASHLIGHT",
        "NOTIFICATIONS",
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "notification": {
        "icon": "./assets/notification-icon.png",
        "color": "#2565A3"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "androidNavigationBar": {
      "barStyle": "light-content"
    },
    "plugins": [
      "expo-sqlite",
      "expo-font",
      "expo-camera",
      "expo-barcode-scanner",
      "expo-splash-screen"
    ],
    "extra": {
      "eas": {
        "projectId": "003365ac-1625-4068-b9b7-70ec42eaba9e"
      }
    },
    "owner": "maximiliano.2024"
  }
}